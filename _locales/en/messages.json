    "CUSTOM_INSTALL_TIP": {
        "message": "All steps here are guid to install backend which will be started in 'Automatically' mode.<br/> \"{FIREFOX_DOWNLOAD_DIRECTORY}\" below means default download directory set in browser Preference (Files and Applications - Downloads)."
    },
    "STEP1_TEXT_1": {
        "message": "Download ScrapBee backend executable to\"<i>{FIREFOX_DOWNLOAD_DIRECTORY}</i>/scrapbee/\", by just click on the button below."
    },
    "STEP1_TEXT_2": {
        "message": "Please ensure https://raw.githubusercontent.com accessable. <br><br>You maybe meet problem that Firefox does not download the program correctly, then you can close Firefox and remove existing executable in\"<i>{FIREFOX_DOWNLOAD_DIRECTORY}</i>/scrapbee/\", start Firefox and download again. (Please use standard version of {Firefox} 60 or later)"
    },
    "STEP2_TEXT_1": {
        "message": "Download installation scripts to \"<i>{FIREFOX_DOWNLOAD_DIRECTORY}</i>/scrapbee/\""
    },
    "STEP3_TEXT_1": {
        "message": "Run installation scripts"
    },
    "STEP3_TEXT_2": {
        "message": "Run the commands in your terminal:"
    },
    "STEP3_TEXT_3": {
        "message": "You maybe need to run the script as administrator"
    },
    "STEP4_TEXT": {
        "message": "Restart {Firefox}"
    },
